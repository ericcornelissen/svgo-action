{
  "name": "svgo-action",
  "version": "3.1.1",
  "private": true,
  "description": "Automatically run SVGO with GitHub Actions",
  "homepage": "https://github.com/marketplace/actions/svgo-action",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.cjs",
  "scripts": {
    "prepare": "is-ci || husky install script/hooks",
    "_clean": "git clean --force -X",
    "_eslint": "eslint --report-unused-disable-directives",
    "audit": "npm audit",
    "audit:prod": "npm audit --omit dev",
    "build": "rollup --config rollup.config.js",
    "clean": "run-p --silent 'clean:*'",
    "clean:reports": "npm run _clean -- ./_reports",
    "clean:temp": "npm run _clean -- ./.cache ./.temp",
    "coverage": "npm run coverage:all",
    "coverage:all": "npm run test:all -- --coverage",
    "coverage:unit": "npm run test:unit -- --coverage",
    "coverage:integration": "npm run test:integration -- --coverage",
    "format": "run-p --silent 'format:*'",
    "format:js": "npm run lint:js -- --fix",
    "format:json": "npm run lint:json -- --fix",
    "format:md": "run-s --silent 'format:md:*'",
    "format:md:code": "npm run lint:md:code -- --fix",
    "format:md:text": "npm run lint:md:text -- --fix",
    "format:ts": "npm run lint:ts -- --fix",
    "format:yml": "npm run lint:yml -- --fix",
    "license-check": "licensee --errors-only",
    "lint": "run-p --silent 'lint:!(sh|docker)'",
    "lint:docker": "docker run -i --rm --mount \"type=bind,source=$(pwd)/.hadolint.yml,target=/.config/hadolint.yaml\" hadolint/hadolint@sha256:9259e253a4e299b50c92006149dd3a171c7ea3c5bd36f060022b5d2c1ff0fbbe < ./.devcontainer/Dockerfile",
    "lint:js": "npm run _eslint -- . --ext .cjs,.js",
    "lint:json": "npm run _eslint -- . --ext .json",
    "lint:md": "run-p --silent 'lint:md:*'",
    "lint:md:code": "npm run _eslint -- *.md **/*.md --ext .md,.yml",
    "lint:md:text": "markdownlint --dot --ignore-path .gitignore .",
    "lint:sh": "shellcheck -e SC1090,SC1091,SC2155 script/*.sh script/hooks/*.sh script/hooks/commit-* script/hooks/pre-*",
    "lint:ts": "npm run _eslint -- . --ext .ts",
    "lint:ws": "editorconfig-checker -disable-max-line-length",
    "lint:yml": "npm run _eslint -- . --ext .yml",
    "test": "npm run test:all",
    "test:all": "jest",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:e2e": "act --job test-e2e",
    "test:mutation": "stryker run stryker.config.js",
    "vet": "depcheck && unimported && type-coverage --at-least 100 --project tsconfig.json --strict"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericcornelissen/svgo-action"
  },
  "bugs": {
    "url": "https://github.com/ericcornelissen/svgo-action/issues"
  },
  "author": {
    "name": "Eric Cornelissen",
    "email": "ericornelissen@gmail.com",
    "url": "https://ericcornelissen.dev/"
  },
  "keywords": [
    "actions",
    "node",
    "svgo"
  ],
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1",
    "eval": "0.1.8",
    "import-cwd": "3.0.0",
    "js-yaml": "4.1.0",
    "minimatch": "5.1.0",
    "svgo-v2": "npm:svgo@2.8.0",
    "svgo-v3": "npm:svgo@3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-json": "5.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "0.1.0",
    "@rollup/plugin-typescript": "9.0.2",
    "@stryker-mutator/core": "6.3.0",
    "@stryker-mutator/jest-runner": "6.3.0",
    "@stryker-mutator/typescript-checker": "6.3.0",
    "@types/jest": "29.2.1",
    "@types/jest-when": "3.5.2",
    "@types/js-yaml": "4.0.5",
    "@types/minimatch": "5.1.2",
    "@types/node": "18.11.8",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "depcheck": "1.4.3",
    "editorconfig-checker": "4.0.2",
    "eslint": "8.26.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-security": "1.5.0",
    "eslint-plugin-yml": "1.2.0",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jest": "29.2.2",
    "jest-when": "3.5.2",
    "licensee": "9.0.0",
    "markdownlint-cli": "0.32.2",
    "npm-run-all": "4.1.5",
    "rollup": "3.2.5",
    "ts-jest": "29.0.3",
    "type-coverage": "2.23.0",
    "typescript": "4.8.4",
    "unimported": "1.22.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.1.2"
  }
}
