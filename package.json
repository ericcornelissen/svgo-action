{
  "name": "svgo-action",
  "version": "4.0.9",
  "private": true,
  "description": "Automatically run SVGO with GitHub Actions",
  "homepage": "https://github.com/marketplace/actions/svgo-action",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.cjs",
  "scripts": {
    "prepare": "husky script/hooks",
    "_clean": "git clean --force -X",
    "_eslint": "eslint --report-unused-disable-directives",
    "audit": "better-npm-audit audit",
    "audit:prod": "better-npm-audit audit --production",
    "build": "rollup --config rollup.config.js",
    "clean": "npm run clean:reports && npm run clean:temp",
    "clean:reports": "npm run _clean -- ./_reports",
    "clean:temp": "npm run _clean -- ./.cache ./.temp",
    "coverage": "npm run coverage:all",
    "coverage:all": "npm run test:all -- --coverage",
    "coverage:integration": "npm run test:integration -- --coverage",
    "coverage:unit": "npm run test:unit -- --coverage",
    "format": "npm run format:js && npm run format:json && npm run format:md && npm run format:ts && npm run format:yml",
    "format:js": "npm run lint:js -- --fix",
    "format:json": "npm run lint:json -- --fix",
    "format:md": "npm run format:md:code && npm run format:md:text",
    "format:md:code": "npm run lint:md:code -- --fix",
    "format:md:text": "npm run lint:md:text -- --fix",
    "format:ts": "npm run lint:ts -- --fix",
    "format:yml": "npm run lint:yml -- --fix",
    "license-check": "licensee --errors-only",
    "lint": "npm run lint:ci && npm run lint:docker && npm run lint:js && npm run lint:json && npm run lint:md && npm run lint:sh && npm run lint:ts && npm run lint:ws && npm run lint:yml",
    "lint:ci": "node script/maybe-run.js actionlint",
    "lint:docker": "node script/maybe-run.js hadolint ./.devcontainer/Dockerfile",
    "lint:js": "npm run _eslint -- . --ext .cjs,.js",
    "lint:json": "npm run _eslint -- . --ext .json",
    "lint:md": "npm run lint:md:code && npm run lint:md:text",
    "lint:md:code": "npm run _eslint -- *.md **/*.md --ext .md,.yml",
    "lint:md:text": "markdownlint --dot --ignore-path .gitignore .",
    "lint:sh": "node script/maybe-run.js shellcheck script/hooks/*.sh script/hooks/commit-* script/hooks/pre-*",
    "lint:ts": "npm run _eslint -- . --ext .ts",
    "lint:ws": "editorconfig-checker -disable-max-line-length",
    "lint:yml": "npm run _eslint -- . --ext .yml",
    "test": "npm run test:all",
    "test:all": "jest",
    "test:e2e": "act --job test-e2e --workflows .github/workflows/check.yml",
    "test:integration": "jest test/integration",
    "test:mutation": "stryker run stryker.config.js",
    "test:unit": "jest test/unit",
    "verify": "npm run license-check && npm run lint && npm run test:unit && npm run vet",
    "vet": "npm run vet:deps && npm run vet:package-lock.json && npm run vet:types",
    "vet:deps": "knip --config .knip.jsonc",
    "vet:package-lock.json": "lockfile-lint",
    "vet:types": "type-coverage --at-least 100 --project tsconfig.json --strict"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ericcornelissen/svgo-action"
  },
  "bugs": {
    "url": "https://github.com/ericcornelissen/svgo-action/issues"
  },
  "author": {
    "name": "Eric Cornelissen",
    "email": "ericornelissen@gmail.com",
    "url": "https://ericcornelissen.dev/"
  },
  "keywords": [
    "actions",
    "node",
    "svgo"
  ],
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0",
    "@actions/glob": "0.4.0",
    "eval": "0.1.8",
    "import-cwd": "3.0.0",
    "svgo-v2": "npm:svgo@2.8.0",
    "svgo-v3": "npm:svgo@3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@stryker-mutator/core": "8.2.6",
    "@stryker-mutator/jest-runner": "8.2.6",
    "@stryker-mutator/typescript-checker": "8.2.6",
    "@types/jest": "29.5.12",
    "@types/jest-when": "3.5.5",
    "@types/node": "20.12.2",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "better-npm-audit": "3.7.3",
    "editorconfig-checker": "5.1.5",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-markdown": "4.0.1",
    "eslint-plugin-mocha": "10.4.1",
    "eslint-plugin-security": "2.1.1",
    "eslint-plugin-yml": "1.13.2",
    "husky": "9.0.11",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-when": "3.6.0",
    "knip": "5.7.0",
    "licensee": "10.0.0",
    "lockfile-lint": "4.13.2",
    "markdownlint-cli": "0.39.0",
    "rollup": "4.13.2",
    "ts-jest": "29.1.2",
    "type-coverage": "2.27.2",
    "typescript": "5.4.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.1.2"
  }
}
